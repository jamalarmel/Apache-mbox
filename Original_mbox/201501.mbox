            public Iterable<Vector> call(String s) {
                String[] split = s.split(",");
                ArrayList<Vector> add = new ArrayList<Vector>();
                if (split.length != 2) {
                    add.add(Vectors.dense(0, 0));
                } else
                {
                    add.add(Vectors.dense(Double.parseDouble(split[0]),
               Double.parseDouble(split[1])));
