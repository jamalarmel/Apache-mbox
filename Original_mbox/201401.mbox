package org.apache.spark.examples

import org.apache.spark._

object HdfsTest {
  def main(args: Array[String]) {
    val sc =3D new SparkContext(args(0), "HdfsTest",
      System.getenv("SPARK_HOME"), =
SparkContext.jarOfClass(this.getClass))
    val file =3D sc.textFile(args(1))
    val mapped =3D file.map(s =3D> s.length).cache()
    for (iter <- 1 to 10) {
      val start =3D System.currentTimeMillis()
      for (x <- mapped) { x + 2 }
      //  println("Processing: " + x)
      val end =3D System.currentTimeMillis()
      println("Iteration " + iter + " took " + (end-start) + " ms")
      mapped.saveAsTextFile("out")
    }
    System.exit(0)
  }
}

